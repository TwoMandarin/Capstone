<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTDMapper3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ProdyTalk.mapper.CalendarMapper">

    <select id="getCalendar" resultType="com.example.ProdyTalk.vo.CalendarVO">
        select * from calendar
    </select>

    <insert id="addEvent" parameterType="com.example.ProdyTalk.vo.CalendarVO">
        insert into calendar (title,start,end,color)
        values (#{title},#{start},#{end},#{color})
    </insert>


</mapper>
